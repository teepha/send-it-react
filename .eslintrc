{
  "parserOptions": {
    "ecmaVersion": 2018
  },
  "env": {
    "node": true,
    "es6": true,
    "mocha": true
  },
  "parser": "babel-eslint",
  "plugins": ["react"],
  "rules": {
    "no-unused-vars": 0,
    "max-len": [1, 120, 2, { "ignoreComments": true }],
    "react/prop-types": [2]
  },
  "extends": ["airbnb-base"]
}
